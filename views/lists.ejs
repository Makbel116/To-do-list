<%- include ("partials/header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    
    <div class="box">
        <% for(var i=0;i<toDoItem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p> <%= toDoItem[i] %></p> 
            </div>
        <% } %>   
        
        <form class="item" action=<%= "/lists/"+listTitle %> method="post">
            <input type="text" name="addList" placeholder= "<%= placeholder %>" autocomplete="off">
            <button type="submit">+</button>
        </form>
    </div>
<%- include ("partials/footer") -%>
